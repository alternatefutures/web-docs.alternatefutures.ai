{
  "name": "@alternatefutures/docs",
  "version": "1.0.0",
  "description": "Documentation for Alternate Futures platform",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "generate:cli": "node scripts/generate-cli-docs.js",
    "generate:sdk": "node scripts/generate-sdk-docs.js",
    "generate:all": "npm run generate:cli && npm run generate:sdk",
    "build": "npm run generate:all && npm run docs:build"
  },
  "keywords": [
    "alternate-futures",
    "documentation",
    "cli",
    "sdk",
    "depin"
  ],
  "author": "Alternate Futures",
  "license": "MIT",
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "markdown-it-container": "^4.0.0",
    "postcss": "^8.5.6",
    "shadcn-vue": "^2.4.3",
    "tailwindcss": "^4.1.18",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "vitepress": "^1.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.562.0",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.6.1",
    "tailwind-merge": "^3.4.0"
  }
}
